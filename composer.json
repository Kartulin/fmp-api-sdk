{
    "name": "kartulin/fmp-api-sdk",
    "description": "FMP-API-SDK",
    "keywords": [
        "kartulin",
        "fmp-api",
        "financialmodelingprep"
    ],
    "homepage": "https://github.com/kartulin/fmp-api-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "kartulin",
            "email": "stalkerxxl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle":"*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "Kartulin\\FmpApiSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kartulin\\FmpApiSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
